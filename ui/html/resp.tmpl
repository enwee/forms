{{define "resp"}}
    {{template "html.start" .}}
        {{range .}}
            {{.Title}} <em>(ver: {{.Version}})</em>
            <table>
                <tr>
                    {{range .TableHeader}}
                        <td>{{.}}</td>
                    {{end}}
                </tr>
                {{range .TableData}}
                    <tr>
                        {{range .Data}}
                            <td>{{.}}</td>
                        {{end}}
                    </tr>
                {{end}}
            </table>
            <br>
        {{end}}
        {{if eq (len .) 0}}<em>No Responses Yet!</em>{{end}}
    {{template "html.end" .}}
{{end}}